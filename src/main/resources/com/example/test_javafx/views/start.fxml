<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<AnchorPane fx:id="rootPane" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.test_javafx.controllers.StartController">
    <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="32.0">
        <padding>
            <Insets bottom="16.0" left="16.0" right="16.0" top="16.0"/>
        </padding>
        <Label text="Example Project">
            <font>
                <Font name="Times New Roman" size="48.0"/>
            </font>
        </Label>
        <Button fx:id="nav_add_section" alignment="CENTER" mnemonicParsing="false" onAction="#navToAddSection"
                prefHeight="42.0" prefWidth="200.0" text="Add Section" textAlignment="CENTER">
            <font>
                <Font name="Times New Roman" size="24.0"/>
            </font>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Button>
        <Button fx:id="nav_lectures_times" mnemonicParsing="false" onAction="#navToLecturesTimes" prefHeight="42.0"
                prefWidth="200.0" text="Lectures times">
            <font>
                <Font name="Times New Roman" size="24.0"/>
            </font>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Button>
        <Button fx:id="nav_enroll_student" mnemonicParsing="false" onAction="#navToEnrollStudent" prefHeight="42.0"
                prefWidth="200.0" text="Enroll Student">
            <font>
                <Font name="Times New Roman" size="24.0"/>
            </font>
            <cursor>
                <Cursor fx:constant="HAND"/>
            </cursor>
        </Button>
    </VBox>

</AnchorPane>
